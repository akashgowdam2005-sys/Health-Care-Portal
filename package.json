{
  "name": "healthcare-portal",
  "version": "1.0.0",
  "description": "A comprehensive healthcare management system for patients, doctors, and pharmacists",
  "private": true,
  "author": "Healthcare Portal Team",
  "license": "MIT",
  "keywords": ["healthcare", "medical", "appointments", "prescriptions", "nextjs", "supabase"],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache",
    "fresh": "npm run clean && npm install && npm run dev",
    "db:types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > lib/database.types.ts"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/ssr": "^0.1.0",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "@tailwindcss/forms": "^0.5.7",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}